<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_main"
    app:startDestination="@id/Start">

    <fragment
        android:id="@+id/Start"
        android:name="androidx.fragment.app.Fragment"
        android:label="@string/label_songs"
        tools:layout="@layout/fragment_empty">

        <action
            android:id="@+id/action_Start_to_Songs"
            app:destination="@id/Songs"
            app:enterAnim="@android:anim/fade_in" />
    </fragment>

    <fragment
        android:id="@+id/Songs"
        android:name="pl.gunock.lyriccast.fragments.SongsFragment"
        android:label="@string/label_songs"
        tools:layout="@layout/fragment_songs">

        <action
            android:id="@+id/action_Songs_to_Setlists"
            app:destination="@id/Setlists" />
    </fragment>

    <fragment
        android:id="@+id/Setlists"
        android:name="pl.gunock.lyriccast.fragments.SetlistsFragment"
        android:label="@string/label_setlists"
        tools:layout="@layout/fragment_setlists">
        <action
            android:id="@+id/action_Setlists_to_Songs"
            app:destination="@id/Songs" />
    </fragment>

</navigation>