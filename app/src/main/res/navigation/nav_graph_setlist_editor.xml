<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_setlist_editor"
    app:startDestination="@id/SetlistEditorFragment">

    <fragment
        android:id="@+id/SetlistEditorSongListFragment"
        android:name="pl.gunock.lyriccast.fragments.SetlistEditorSongListFragment"
        android:label="@string/songs"
        tools:layout="@layout/fragment_song_list">

        <action
            android:id="@+id/action_SetlistEditorSongListFragment_to_SetlistEditorFragment"
            app:destination="@id/SetlistEditorFragment" />
        <argument
            android:name="selectedSongs"
            app:argType="string[]" />
        <argument
            android:name="setlistName"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/SetlistEditorFragment"
        android:name="pl.gunock.lyriccast.fragments.SetlistEditorFragment"
        android:label="@string/songs"
        tools:layout="@layout/fragment_setlist_editor">

        <action
            android:id="@+id/action_SetlistEditorFragment_to_SetlistEditorSongListFragment"
            app:destination="@id/SetlistEditorSongListFragment" />
        <argument
            android:name="selectedSongs"
            android:defaultValue="@null"
            app:argType="string[]"
            app:nullable="true" />
        <argument
            android:name="setlistName"
            android:defaultValue="@null"
            app:argType="string"
            app:nullable="true" />
    </fragment>
</navigation>